{
  "manifest_version": 3,
  "name": "Stars logger",
  "version": "1.0",
  "description": "Log the phone number of contacts on WhatsApp Web.",
  "permissions": [
    "scripting"
  ],
  "host_permissions": [
    "https://web.whatsapp.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://web.whatsapp.com/*"
      ],
      "js": [
        "content.js",
        "login.js"
      ]
    }
  ]
}